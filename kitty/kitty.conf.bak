kitty.conf.bak

font_family      JetBrainsMono Nerd Font
bold_font        auto
italic_font      auto
bold_italic_font auto
font_size 12

symbol_map U+E5FA-U+E6AC JetBrainsMono Nerd Font
symbol_map U+E700-U+E7C5 JetBrainsMono Nerd Font
symbol_map U+F000-U+F2E0 JetBrainsMono Nerd Font
symbol_map U+E200-U+E2A9 JetBrainsMono Nerd Font
symbol_map U+F0001-U+F1AF0 JetBrainsMono Nerd Font
symbol_map U+E300-U+E3E3 JetBrainsMono Nerd Font
symbol_map U+F400-U+F532,U+2665,U+26A1 JetBrainsMono Nerd Font
symbol_map U+E0A0-U+E0A2,U+E0B0-U+E0B3 JetBrainsMono Nerd Font
symbol_map U+E0A3,U+E0B4-U+E0C8,U+E0CA,U+E0CC-U+E0D4 JetBrainsMono Nerd Font
symbol_map U+23FB-U+23FE,U+2B58 JetBrainsMono Nerd Font
symbol_map U+F300-U+F32F JetBrainsMono Nerd Font
symbol_map U+E000-U+E00A JetBrainsMono Nerd Font
symbol_map U+EA60-U+EBEB JetBrainsMono Nerd Font
symbol_map U+E276C-U+E2771 JetBrainsMono Nerd Font # Heavy Angle Brackets
symbol_map U+2500-U+259F JetBrainsMono Nerd Font # Box Drawing

# window_padding_width 10

disable_ligatures always
tab_bar_edge top
tab_bar_min_tabs 2
tab_bar_style separator
tab_separator " | "
tab_powerline_style slanted
tab_title_template "{fmt.fg.red}{bell_symbol}{activity_symbol}{fmt.fg.tab}{index}"

background_image $HOME/.config/kitty/gojo.png
# background_image $HOME/Downloads/sad-girl-anime-girl-6144x4096-10021.jpg
background_blur 10
# macos_traditional_fullscreen yes
background_image_layout cscaled
background_opacity 0.8
background_tint 0.94
background_tint_gaps 10
# dim_opacity 1
background_image_linear yes
# dynamic_background_opacity no
include current-theme.conf


# BEGIN_KITTY_THEME
# Catppuccin-Mocha
include current-theme.conf
# END_KITTY_THEME
